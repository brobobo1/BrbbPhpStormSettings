<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Close">
        <Item>
          <IdeaActionID>CloseContent</IdeaActionID>
          <ShortCut>'Ctrl+F4'</ShortCut>
          <Description>Close</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'Ctrl+C' or 'Ctrl+Insert'</ShortCut>
          <Description>Copy</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Database">
        <Item>
          <IdeaActionID>ActivateDatabaseToolWindow</IdeaActionID>
          <ShortCut>'Alt+3'</ShortCut>
          <Description>Database</Description>
          <Count>7</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Open Console">
        <Item>
          <IdeaActionID>Jdbc.OpenConsole</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F10'</ShortCut>
          <Description>Open Console</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Start Docker'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Shift+F10'</ShortCut>
          <Description>Run 'Start Docker'</Description>
          <Count>18</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'index.php'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Shift+F10'</ShortCut>
          <Description>Run 'index.php'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>24</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'Alt+F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Update Project...">
        <Item>
          <IdeaActionID>Vcs.UpdateProject</IdeaActionID>
          <ShortCut>'Ctrl+T'</ShortCut>
          <Description>Update Project...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'Alt+9'</ShortCut>
          <Description>Version Control</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>